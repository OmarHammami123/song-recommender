# Copy this file to .env and fill in your actual values

# Kaggle API Credentials
# Get these from your Kaggle account settings: https://www.kaggle.com/settings/account
KAGGLE_USERNAME=your_kaggle_username
KAGGLE_KEY=your_kaggle_api_key

# Optional: Spotify API Credentials (for future features)
# Get these from: https://developer.spotify.com/dashboard/applications
SPOTIFY_CLIENT_ID=your_spotify_client_id
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret

# Application Configuration
# Environment: development, staging, production
APP_ENV=development

# Data Processing Configuration
MAX_SONGS_LIMIT=100000
BATCH_SIZE=1000
